{
    "routes": [
        {
            "route": "/login",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/register",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/register/finish",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/dashboard",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/assignments",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/assignment",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/profile",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/members",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/users",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "responseOverrides": {
        "400": {
            "rewrite": "/"
        },
        "401": {
            "redirect": "/"
        },
        "403": {
            "rewrite": "/"
        },
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "mimeTypes": {
        ".js": "text/javascript",
        ".mjs": "text/javascript",
        ".jsx": "text/javascript",
        ".tsx": "text/javascript",
        ".ts": "text/javascript",
        ".html": "text/html",
        ".json": "text/json",
        ".css": "text/css"
    }
  }